{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./build",
      "paths": {
        "@/*": ["./src/server/*"]
      }
    },
    "include": ["src/server/*.ts"],
    "exclude": ["node_modules"],
    
  }
  